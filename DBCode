//MEMBER

CREATE TABLE MEMBER 
(
  PID VARCHAR2(20) NOT NULL 
, PW VARCHAR2(20) NOT NULL 
, NAME VARCHAR2(20) NOT NULL 
, JUMIN1 VARCHAR2(6) NOT NULL 
, JUMIN2 VARCHAR2(7) NOT NULL 
, TEL VARCHAR2(13) 
, DID VARCHAR2(20) 
, CONSTRAINT MEMBER_PK PRIMARY KEY 
  (
    ID 
  )
  ENABLE 
);


//BUILDING

CREATE TABLE BUILDING 
(
  NUM NUMBER NOT NULL 
, BNAME VARCHAR2(20) NOT NULL 
, CONSTRAINT BUILDING_PK PRIMARY KEY 
  (
    NUM 
  )
  ENABLE 
);

ALTER TABLE BUILDING
ADD CONSTRAINT BUILDING_UK1 UNIQUE 
(
  BNAME 
)
ENABLE;


//DOCTOR

CREATE TABLE DOCTOR 
(
  DID VARCHAR2(20) NOT NULL 
, PW VARCHAR2(20) NOT NULL 
, NAME VARCHAR2(20) NOT NULL
, TEL VARCHAR2(13) 
, DAY VARCHAR2(20) NOT NULL 
, BNAME VARCHAR2(20) NOT NULL 
, CONSTRAINT DOCTOR_PK PRIMARY KEY 
  (
    DID 
  )
  ENABLE 
);


//제약조건

ALTER TABLE MEMBER
ADD CONSTRAINT MEMBER_FK1 FOREIGN KEY
(
  DID 
)
REFERENCES DOCTOR
(
  DID 
)
ENABLE;


ALTER TABLE DOCTOR
ADD CONSTRAINT DOCTOR_FK1 FOREIGN KEY
(
  BNAME 
)
REFERENCES BUILDING
(
  BNAME 
)
ENABLE;

